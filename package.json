{
  "name": "time-tracking",
  "displayName": "Time Tracking",
  "description": "Tracking programmers' time",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "timeTracking.open",
        "title": "Open Time Tracking"
      },
      {
        "command": "timeTracking.dump",
        "title": "Dump Time Tracking data to a file"
      }
    ],
    "configuration": {
      "title": "Time Tracking",
      "properties": {
        "timeTracking.samplingIntervalLength": {
          "type": "int",
          "default": 30,
          "minimum": 10,
          "maximum": 600,
          "description": "Length of the sampling interval (in seconds)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "@types/luxon": "^2.0.8",
    "chart.js": "^3.6.0",
    "luxon": "^2.2.0"
  }
}
