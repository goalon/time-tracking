{
  "name": "mimuw-mb-tt-time-tracking",
  "displayName": "[MIMUW-MB-TT] Time Tracking",
  "description": "Tracking programmers' time and gathering data for the research",
  "repository": "github:goalon/time-tracking",
  "license": "SEE LICENSE IN LICENSE",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Visualization",
    "Data Science",
    "Programming Languages",
    "Other"
  ],
  "homepage": "https://github.com/goalon/time-tracking",
  "bugs": {
    "url": "https://github.com/goalon/time-tracking/issues",
    "email": "m.bajorek2@student.uw.edu.pl"
  },
  "icon": "https://github.com/goalon/time-tracking/blob/main/static/icon.png",
  "galleryBanner": {
    "color": "#00008B",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "timeTracking.open",
        "title": "[MIMUW-MB-TT] Open Time Tracking dashboard",
        "when": "timeTracking.showCommand"
      },
      {
        "command": "timeTracking.dump",
        "title": "[MIMUW-MB-TT] Save Time Tracking data",
        "when": "inDebugMode && timeTracking.showCommand"
      },
      {
        "command": "timeTracking.upload",
        "title": "[MIMUW-MB-TT] Upload Time Tracking data",
        "when": "inDebugMode && timeTracking.showCommand"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/luxon": "^2.0.8",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/nunjucks": "^3.2.1",
    "@types/tar": "^6.1.1",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "@vscode/codicons": "^0.0.32",
    "chart.js": "^3.6.0",
    "chartjs-adapter-luxon": "^1.2.0",
    "crypto-js": "^4.1.1",
    "dropbox": "^10.26.0",
    "fullcalendar": "^5.11.3",
    "luxon": "^2.2.0",
    "node-fetch": "2.6.7",
    "nunjucks": "^3.2.3",
    "tar": "^6.1.11",
    "uuid": "^8.3.2"
  }
}
